{
  "name": "@yoyodyne/foundation-x",
  "version": "1.0.0",
  "description": "Proposed Revamp of the In-House Design System",
  "keywords": [
    "demo",
    "design system",
    "react",
    "vite",
    "storybook",
    "tailwindcss",
    "template",
    "prototype"
  ],
  "license": "BSD-3-Clause",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "author": {
    "name": "Tim Goshinski",
    "email": "timothy.goshinski@kiewit.com",
    "url": "https://kiewit.dev"
  },
  "homepage": "https://github.com/yoyodyne-build/foundation-x",
  "bugs": {
    "url": "https://github.com/yoyodyne-build/foundation-x/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yoyodyne-build/foundation-x.git"
  },
  "scripts": {
    "commit": "czg",
    "check:commit": "commitlint --edit",
    "check:format": "prettier --check .",
    "check:exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "check:lockfile": "node ./scripts/validate-lockfile.cjs",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "format": "run-p format:*",
    "format:code": "prettier --write .",
    "format:styles": "stylelint \"./src/**/*.css\" --fix",
    "format:ts": "eslint ./src/**/**.ts* --fix",
    "lint": "run-p lint:*",
    "lint:ts": "eslint ./src/**/**.ts*",
    "lint:styles": "stylelint \"./src/**/*.css\"",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "release:local": "changeset version && changeset publish",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "volta": {
    "node": "22.11.0"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.5.4"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@changesets/cli": "^2.27.9",
    "@chromatic-com/storybook": "^1.9.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.13.0",
    "@storybook/addon-essentials": "^8.3.6",
    "@storybook/addon-interactions": "^8.3.6",
    "@storybook/addon-links": "^8.3.6",
    "@storybook/addon-onboarding": "^8.3.6",
    "@storybook/blocks": "^8.3.6",
    "@storybook/react": "^8.3.6",
    "@storybook/react-vite": "^8.3.6",
    "@storybook/test": "^8.3.6",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint__js": "~8.42.3",
    "@types/eslint-config-prettier": "~6.11.3",
    "@types/eslint-plugin-jsx-a11y": "~6.9.0",
    "@types/jsdom": "~21.1.7",
    "@types/lint-staged": "~13.3.0",
    "@types/node": "^20.17.1",
    "@types/postcss-url": "~10.0.4",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-istanbul": "^2.1.4",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "czg": "^1.10.1",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.13",
    "eslint-plugin-storybook": "^0.10.1",
    "fast-glob": "^3.3.2",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "postcss-url": "^10.1.3",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "storybook": "^8.3.6",
    "stylelint": "^16.10.0",
    "stylelint-config-recommended": "^14.0.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.4.14",
    "tailwindcss-debug-screens": "^2.2.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.10.0",
    "vite": "^5.4.9",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^2.1.4"
  }
}
